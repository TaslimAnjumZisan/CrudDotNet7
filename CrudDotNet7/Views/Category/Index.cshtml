@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Category Master</h1>
<table>
    <tr>
        <td>
            <a asp-controller="Category" asp-action="Create">Create new Category</a>
        </td>
    </tr>
</table>

<table style="width:500px" border="1">
    <tr>
        <td style="width:200px" border="1">Category Name</td>
        <td style="width:200px" border="1">Display Order</td>
    </tr>
    @foreach(var obj in Model)
    {
        <tr>
            <td style="width:200px">@obj.Name</td>
            <td style="width:200px">@obj.Display</td>
        </tr>
    }
</table>